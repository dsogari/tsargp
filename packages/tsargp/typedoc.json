    "formatBoolean",
    "formatNumber",
    "formatOption",
    "formatParam",
    "formatRegExp",
    "formatString",
    "formatTerm",
    "formatURL",
    "Misc",
    "Resolve",
    "SingleDataType",
    "Writable"
  ]
}

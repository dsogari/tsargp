{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["../tsargp/src/index.ts"],
  "tsconfig": "../tsargp/src/tsconfig.json",
  "favicon": "favicon.svg",
  "out": "public/api",
  "githubPages": false,
  "excludeExternals": true,
  "treatWarningsAsErrors": true,
  "intentionallyNotExported": [
    "Alias",
    "Enumerate",
    "PartialWithDepth",
    "Resolve",
    "Args",
    "Promissory",
    "OptionKeyType",
    "OptionDataType",
    "WithRequired",
    "WithDefault",
    "WithExample",
    "WithParamName",
    "WithChoices",
    "WithRegex"
  ]
}

{
  "$schema": "node_modules/@cspell/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["cpp", "typescript"],
  "ignoreWords": [
    "Sogari",
    "lockb",
    "tsargp",
    "overlined",
    "outdir",
    "publint",
    "trulysimple",
    "nextra",
    "niladic",
    "println",
    "compspec",
    "svgr",
    "codemirror",
    "CODEOWNERS"
  ],
  "ignoreRegExpList": ["@[\\w-]+"],
  "ignorePaths": ["*.lockb", ".prettierignore", "CODEOWNERS"],
  "useGitignore": true
}

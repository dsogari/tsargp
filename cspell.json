{
  "$schema": "node_modules/@cspell/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["typescript"],
  "words": ["overlined", "niladic", "polyadic"],
  "ignoreWords": [
    "tsargp",
    "nextra",
    "publint",
    "println",
    "compspec",
    "compdef",
    "oclif",
    "IIFE",
    "mult",
    "vals",
    "descr",
    "conv",
    "punct",
    "charmap",
    "ruleset",
    "rulesets"
  ],
  "ignoreRegExpList": ["gh-username", "ga-measurement-id"],
  "ignorePaths": [
    "*.lockb",
    "*.json",
    ".git",
    ".prettierignore",
    "CODEOWNERS",
    "*.{js,ts,jsx,tsx}"
  ],
  "useGitignore": true,
  "enableGlobDot": true,
  "patterns": [
    {
      "name": "gh-username",
      "pattern": "/@[\\w-]+/g"
    },
    {
      "name": "ga-measurement-id",
      "pattern": "/G-[\\w]+/g"
    }
  ]
}

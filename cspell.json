{
  "$schema": "node_modules/@cspell/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["cpp", "typescript"],
  "ignoreWords": [
    "Sogari",
    "lockb",
    "tsargp",
    "overlined",
    "outdir",
    "publint",
    "trulysimple",
    "nextra",
    "niladic",
    "println",
    "compspec",
    "svgr",
    "oclif",
    "codemirror",
    "CODEOWNERS"
  ],
  "ignoreRegExpList": ["gh-username", "ga-measurement-id"],
  "ignorePaths": ["*.lockb", ".prettierignore", "CODEOWNERS"],
  "useGitignore": true,
  "patterns": [
    {
      "name": "gh-username",
      "pattern": "@[\\w-]+"
    },
    {
      "name": "ga-measurement-id",
      "pattern": "G-[\\w-]+"
    }
  ]
}

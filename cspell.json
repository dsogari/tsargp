{
  "$schema": "node_modules/@cspell/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["cpp", "typescript"],
  "words": ["uncategorized"],
  "flagWords": ["contraint"],
  "ignoreWords": [
    "Sogari",
    "lockb",
    "tsargp",
    "overlined",
    "outdir",
    "publint",
    "trulysimple",
    "nextra",
    "niladic",
    "polyadic",
    "println",
    "compspec",
    "svgr",
    "oclif",
    "codemirror",
    "IIFE"
  ],
  "ignoreRegExpList": ["gh-username", "ga-measurement-id"],
  "ignorePaths": [
    "*.lockb",
    ".prettierignore",
    "CODEOWNERS",
    "pre.json",
    "cspell.json",
    ".git",
    "*.{js,ts,jsx,tsx}"
  ],
  "useGitignore": true,
  "enableGlobDot": true,
  "patterns": [
    {
      "name": "gh-username",
      "pattern": "/@[\\w-]+/g"
    },
    {
      "name": "ga-measurement-id",
      "pattern": "/G-[\\w]+/g"
    }
  ]
}

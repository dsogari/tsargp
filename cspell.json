{
  "$schema": "node_modules/@cspell/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["typescript"],
  "words": ["overlined", "niladic", "polyadic", "satisfiability"],
  "ignoreWords": [
    "Diego",
    "Sogari",
    "tsargp",
    "nextra",
    "nushell",
    "publint",
    "println",
    "compspec",
    "compdef",
    "oclif",
    "twoslash",
    "mult",
    "vals",
    "descr",
    "conv",
    "punct",
    "charmap",
    "ruleset",
    "rulesets"
  ],
  "ignoreRegExpList": ["gh-username", "ga-measurement-id"],
  "ignorePaths": [
    "*.{js,ts,jsx,tsx,lock,json,jsonc,svg,git,prettierignore}",
    "**/.next/",
    "**/dist/",
    "**/public/"
  ],
  "useGitignore": true,
  "enableGlobDot": true,
  "patterns": [
    {
      "name": "gh-username",
      "pattern": "/@[\\w-]+/g"
    },
    {
      "name": "ga-measurement-id",
      "pattern": "/G-[\\w]+/g"
    }
  ]
}
